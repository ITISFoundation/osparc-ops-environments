{
"files.associations": {
    ".*rc": "ini",
    ".env*": "ini",
    "*.config": "ini",
    "**/requirements/*.in": "pip-requirements",
    "**/requirements/*.txt": "pip-requirements",
    "*logs.txt": "log",
    "*.logs*": "log",
    "*Makefile": "makefile",
    "docker-compose*.yml": "dockerfile",
    "Dockerfile*": "dockerfile"
  }
}